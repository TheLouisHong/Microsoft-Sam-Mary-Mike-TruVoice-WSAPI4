\par // globals
\par PCVoiceDictation    gpCVDct;
\par WCHAR               DictationBuffer[MAXBUFFER];
\par 
\par // Voice Dictation notification sink*********************************************************
\par STDMETHODIMP CTestNotifySink::TextChanged(DWORD dwReason)
\par \{
\par     DWORD newStart, newEnd, oldStart, oldEnd;
\par     SDATA Data;
\par     HRESULT hRes;
\par     Data.pData = NULL;
\par     gpCVDct->Lock();
\par     hRes = gpCVDct->GetChanges(&newStart, &newEnd, &oldStart, &oldEnd);
\par     if (hRes) 
\par         return -1;
\par     if(oldStart < oldEnd) \{    // then delete text from buffer
\par             memmove(
\par                 &DictationBuffer[oldStart],
\par                 &DictationBuffer[oldEnd],
\par                 MAXBUFFER -- oldEnd * sizeof(WCHAR));
\par     \}
\par     if(newEnd > newStart) \{
\par         gpCVDct->TextGet(newStart, newEnd -- newStart, &Data);
\par         if(Data.pData) \{       // then insert text into buffer
\par             memmove(
\par                 &DictationBuffer[newStart + (Data.dwSize -- 1) / sizeof(WCHAR)],
\par                 &DictationBuffer[newStart],
\par                 MAXBUFFER -- (newStart * sizeof(WCHAR) + Data.dwSize));
\par             memmove(
\par                 &DictationBuffer[newStart],
\par                 (WCHAR *)Data.pData,
\par                 Data.dwSize -- sizeof(WCHAR));                
\par         \}
\par     \}
\par     system("cls");             //clear the screen
\par     wprintf(DictationBuffer);
\par     if(Data.pData)
\par         CoTaskMemFree(Data.pData);
\par     gpCVDct->UnLock();
\par     return NOERROR;
\par \}}{\cell }\pard\plain \widctlpar\intbl\adjustright \fs20\cf14\cgrid {\cf0 \row }\pard\plain \s16\sa120\sl-240\slmult0\widctlpar\adjustright \fs20\cgrid {The application's callbacks in }{\b CTestNotifySink}{
